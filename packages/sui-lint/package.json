{
  "name": "@s-ui/lint",
  "version": "3.14.0",
  "description": "Linting CLI for sui packages",
  "main": "./bin/sui-lint.js",
  "bin": {
    "sui-lint": "./bin/sui-lint.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "sui-lint js",
